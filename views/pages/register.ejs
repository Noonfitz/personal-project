<%- include('../partials/head') -%>

 
<%- include('../partials/nav') -%>


<div class="container">
  <div class="row">
      <div class='col-md-3'></div>
      <div class="col-md-6">
          <div class="login-box well">
                  <form action="/register" method="POST">
                      <h3>Register</h3>
                      <div class="form-group">
                          <label for="username-email">Username</label>
                          <input value='' id="username-email" name="username" placeholder="Username" type="text" class="form-control" required/>
                      </div>
                      <div class="form-group">
                          <label for="password">Password</label>
                          <input  type="password" id="password" name="password" placeholder="Password" class="form-control" required/>
                      </div>
                      
                      <div class="form-group">
                          <input type="submit" class="pattern-button" value="Login" />
                      </div>

                      <p class="regis-google"><a href="/auth/google">Sign Up with Google</a></p>
                    
                  </form>
              
          </div>
      </div>
      <div class='col-md-3'></div>
  </div>
</div>




<%- include('../partials/footer') -%>